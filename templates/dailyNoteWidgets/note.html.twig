<div class="card col">
    <div class=" row card-body">
        <h3>Note</h1>

            <table class="table">
                <thead>

                <tr>
                    <th>Name</th>
                    <th>Description</th>
                </tr>
                </thead>
                <tbody>
                {% for note in notes %}
                    <tr>
                        <td scope="row">{{ note.name }}</td>
                        <td>{{ note.description }}</td>
                    </tr>

                {% endfor %}
                </tbody>
            </table>

    </div>
</div>

</div>
